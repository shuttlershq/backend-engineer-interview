{
   "name": "antarctica",
   "private": true,
   "version": "1.0.0",
   "description": "Authentication microservice",
   "main": "index.js",
   "scripts": {
      "build": "rimraf dist && tsc",
      "preserve": "npm run build",
      "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/src/index.js\"",
      "prestart": "npm run build",
      "start": "ts-node src/index.ts",
      "lint": "eslint . --ext .ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./src/test/config/jest-e2e.json --runInBand"
   },
   "keywords": [
      "shipeasy",
      "microservice"
   ],
   "author": "Shipeasy",
   "license": "ISC",
   "dependencies": {
      "@types/body-parser": "^1.19.2",
      "@types/bunyan": "^1.8.8",
      "@types/bunyan-format": "^0.2.4",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-rate-limit": "^5.1.3",
      "@types/express-requests-logger": "^3.0.1",
      "@types/express-serve-static-core": "^4.17.26",
      "@types/node": "^16.11.11",
      "@types/socket.io": "^3.0.2",
      "@types/uuid": "^8.3.3",
      "@typescript-eslint/eslint-plugin": "^4.29.2",
      "@typescript-eslint/parser": "^4.29.2",
      "bcrypt": "^5.0.1",
      "body-parser": "^1.19.0",
      "bunyan": "^1.8.15",
      "bunyan-format": "^0.2.1",
      "cors": "^2.8.5",
      "cross-env": "^7.0.3",
      "dotenv": "^10.0.0",
      "eslint": "^7.32.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^3.4.1",
      "express": "^4.17.1",
      "express-rate-limit": "^5.5.1",
      "express-requests-logger": "^3.0.4",
      "geojson": "^0.5.0",
      "helmet": "^4.6.0",
      "joi": "^17.5.0",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.14.1",
      "pg": "^8.7.1",
      "prettier": "^2.5.0",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.2",
      "socket.io": "^4.4.0",
      "typeorm": "^0.2.38",
      "uuid": "^8.3.2"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/geojson": "^7946.0.8",
      "@types/jest": "^27.0.3",
      "@types/jsonwebtoken": "^8.5.6",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.11",
      "concurrently": "^6.4.0",
      "factory-girl": "^5.0.4",
      "jest": "^27.4.4",
      "nodemon": "^2.0.15",
      "supertest": "^6.1.6",
      "ts-jest": "^27.1.1",
      "ts-node": "3.3.0",
      "typescript": "^4.5.2"
   }
}
